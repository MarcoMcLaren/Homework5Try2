@model IEnumerable<Homework5Try2.Models.borrows>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Number
        </th>
        <th>
            Taken Date
        </th>
        <th>
            Brought Date
        </th>
        <th>
           Name
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.borrowId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.takenDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.broughtDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.students.name)
        </td>
    </tr>
}

</table>
